{
  "name": "ds2-js",
  "description": "Devil Survivor 2 RB Save Editor",
  "author": "Douman",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DoumanAsh/ds2-js"
  },
  "scripts": {
    "lint": "eslint --ext .jsx --ext .js app/ test/",
    "start": "brunch watch --server",
    "build": "brunch build --production",
    "test": "node --harmony ./node_modules/ava/cli.js --verbose"
  },
  "dependencies": {
    "file-saver": "^1.3.3",
    "inferno": "*",
    "inferno-component": "*"
  },
  "devDependencies": {
    "eslint": "^3.14.1",
    "ava": "*",
    "eslint-plugin-inferno": "*",
    "babel-plugin-inferno": "*",
    "babel-preset-es2015": "*",
    "brunch": "*",
    "autoprefixer-stylus": "*",
    "babel-brunch": "*",
    "clean-css-brunch": "*",
    "css-brunch": "*",
    "eslint-brunch": "*",
    "jade-brunch": "*",
    "rollup-brunch": "*",
    "stylus-brunch": "*",
    "auto-reload-brunch": "*",
    "uglify-js-brunch": "*"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "app/**/*.js"
    ],
    "failFast": true
  }
}
